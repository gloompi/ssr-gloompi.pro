.app
  position: relative
  width: 100%
  min-height: 100vh
  color #fff
  background #000
  background-size 120% auto
  &:after
    content: ''
    position: absolute
    width: 100%
    height: 100%
    left 0
    top: 0
    background-color rgba(0, 0, 0, .7)

.app__main
  position: relative
  z-index: 200